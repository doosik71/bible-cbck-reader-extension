{
  "manifest_version": 3,
  "name": "Bible.cbck Reader",
  "description": "Bible.cbck Reader",
  "version": "0.0.1",
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "images/icon.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://bible.cbck.or.kr/Knb/*"
      ],
      "js": [
        "scripts/content.js"
      ]
    }
  ]
}